,UAjBA,wBAiBA;AAAA;;AACA;AACA;AAEA,UAAA,KAAA,GAAA,iBAAA;AACA,UAAA,IAAA,GAAA;AACA,QAAA,KAAA,EAAA;AADA,OAAA;AAGA,MAAA,OAAA,CAAA,cAAA,CAAA,KAAA,EAAA,UAAA,GAAA,EAAA;AACA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CAAA,MAAA,CAAA,OAAA,CAFA,CAGA;;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,aAAA;AAEA,OANA,EAMA,UAAA,KAAA,EAAA,CACA;AACA,OARA;AASA,KAlCA;AAmCA,IAAA,MAnCA,oBAmCA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,YAAA,EADA,CAEA;;AAGA,MAAA,QAAA,CAAA,aAAA,CAAA,KAAA,aAAA,EAAA,UAAA,GAAA,EAAA;AACA;AACA;AAEA,QAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAJA,CAKA;AACA;;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,iBAAA,CAPA,CAOA;AAGA;;AACA,QAAA,OAAA,CAAA,aAAA,CAAA,MAAA,CAAA,KAAA,EAAA,MAAA,CAAA,QAAA,EAAA,UAAA,GAAA,EAAA;AACA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,cAAA,GAAA,CAAA,MAAA,IAAA,IAAA,EACA,OAAA,CAAA,GAAA,CAAA,aAAA;;AACA,UAAA,MAAA,CAAA,UAAA;AACA,SANA,EAMA,UAAA,KAAA,EAAA;AACA,UAAA,KAAA,CAAA,YAAA,CAAA;AACA,SARA;AAUA,OArBA,EAqBA,UAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,eAAA,CAAA;AACA,OAvBA;AAyBA,KAjEA;AAkEA,IAAA,cAlEA,0BAkEA,KAlEA,EAkEA;AACA,WAAA,aAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,KApEA;AAqEA,IAAA,SArEA,qBAqEA,MArEA,EAqEA;AACA,WAAA,MAAA,GAAA,wBAAA,MAAA,GAAA,QAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,MAAA;AACA,KAxEA;AAyEA,IAAA,QAzEA,sBAyEA;AAEA,WACA,MADA,CAEA,QAFA,CAEA,WAFA;AAIA,KA/EA;AA+EA,IAAA,IA/EA,kBA+EA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA;AAjFA,GAZA;AA+FA,EAAA,IAAA,EAAA;AAAA,WAAA;AACA,MAAA,OAAA,EAAA,CAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,KAAA,CADA;AAEA,MAAA,IAAA,EAAA,EAFA;AAGA,MAAA,GAAA,EAAA,EAHA;AAIA,MAAA,MAAA,EAAA,EAJA;AAKA,MAAA,UAAA,EAAA,EALA;AAMA,MAAA,OAAA,EAAA,EANA;AAOA,MAAA,OAAA,EAAA,EAPA;AAQA,MAAA,OAAA,EAAA,EARA;AASA,MAAA,aAAA,EAAA,EATA;AAUA,MAAA,MAAA,EAAA,EAVA;AAWA,MAAA,IAAA,EAAA,EAXA;AAYA,MAAA,KAAA,EAAA,EAZA;AAaA,MAAA,MAAA,EAAA,EAbA;AAcA,MAAA,KAAA,EAAA,EAdA;AAeA,MAAA,QAAA,EAAA,EAfA;AAgBA,MAAA,IAAA,EAAA,CACA;AAAA,QAAA,QAAA,EAAA,IAAA;AAAA,QAAA,GAAA,EAAA,EAAA;AAAA,QAAA,UAAA,EAAA,WAAA;AAAA,QAAA,SAAA,EAAA;AAAA,OADA,EAEA;AAAA,QAAA,QAAA,EAAA,KAAA;AAAA,QAAA,GAAA,EAAA,EAAA;AAAA,QAAA,UAAA,EAAA,QAAA;AAAA,QAAA,SAAA,EAAA;AAAA,OAFA,EAGA;AAAA,QAAA,QAAA,EAAA,KAAA;AAAA,QAAA,GAAA,EAAA,EAAA;AAAA,QAAA,UAAA,EAAA,QAAA;AAAA,QAAA,SAAA,EAAA;AAAA,OAHA,EAIA;AAAA,QAAA,QAAA,EAAA,IAAA;AAAA,QAAA,GAAA,EAAA,EAAA;AAAA,QAAA,UAAA,EAAA,MAAA;AAAA,QAAA,SAAA,EAAA;AAAA,OAJA,CAhBA;AAqBA;AACA,MAAA,OAAA,EAAA;AAtBA,KAAA;AAAA;AA/FA,CAAA","sourcesContent":["<template>\r\n  <div class=\"signup-content\">\r\n      <div class=\"input-with-label\">\r\n      \r\n      <!-- 리뷰 제목 -->\r\n      <input id=\"reviewTitle\" placeholder=\"리뷰 제목을 작성해주세요.\"\r\n                       type=\"text\" style=\"\r\n            background-color: white;\r\n            color: black;\r\n            border: 2px solid #008Cy\r\n            BA;\"/>\r\n      <label for=\"reviewTitle\">리뷰 제목</label>\r\n\r\n      <!-- 해시 태그 모달 -->\r\n      <b-button id=\"show-btn\"\r\n                    style=\"width: 50%;height: 15%;color: cornflowerblue;background-color: aliceblue; margin-bottom: 10px;\"\r\n                    @click=\"$bvModal.show('bv-modal-example')\">HashTag</b-button>\r\n            <b-modal style=\"text-align: center; margin-bottom: 10px;\" id=\"bv-modal-example\" hide-footer>\r\n                    <template v-slot:modal-title>HashTag</template>\r\n                    <div class=\"d-block text-center\">\r\n                      <h3>지역</h3>\r\n                            <!-- <div v-for = \"fa in this.fixArea\" v-bind:key=fa>\r\n                                    <input type=\"checkbox\" id=\"fa\" value=\"fa\" v-model=\"area\">\r\n                                    <label for=\"fa\">{{fa}}</label>\r\n                            </div> -->\r\n                            <input type=\"checkbox\" id=\"gangnam\" value=\"강남\" v-model=\"area\" v-on:click= \"check_area()\">\r\n                            <label for=\"gangnam\">강남</label>\r\n                            <input type=\"checkbox\" id=\"jongro\" value=\"종로\" v-model=\"area\" v-on:click= \"check_area()\">\r\n                            <label for=\"jongro\">종로</label>\r\n                            <input type=\"checkbox\" id=\"jamsil\" value=\"잠실\" v-model=\"area\" v-on:click= \"check_area()\">\r\n                            <label for=\"jamsil\">잠실</label>\r\n                            <br>\r\n                            <span>체크 : {{ area}}</span>\r\n                      <h3>연령</h3>\r\n                            <input type=\"checkbox\" id=\"ten\" value=\"10대\" v-model=\"age\" v-on:click= \"check_age()\">\r\n                            <label for=\"ten\">10대</label>\r\n                            <input type=\"checkbox\" id=\"twienty\" value=\"20대\" v-model=\"age\" v-on:click= \"check_age()\">\r\n                            <label for=\"twienty\">20대</label>\r\n                            <input type=\"checkbox\" id=\"thirty\" value=\"30대\" v-model=\"age\" v-on:click= \"check_age()\">\r\n                            <label for=\"thirty\">30대</label>\r\n                            <br>\r\n                            <span>체크 : {{age}}</span>\r\n                      <h3>성별</h3>\r\n                            <input type=\"checkbox\" id=\"man\" value=\"남\" v-model=\"gender\" v-on:click= \"check_gender()\">\r\n                            <label for=\"man\">남</label>\r\n                            <input type=\"checkbox\" id=\"girl\" value=\"여\" v-model=\"gender\" v-on:click= \"check_gender()\">\r\n                            <label for=\"girl\">여</label>\r\n                            <br>\r\n                            <span>체크 : {{gender}}</span>\r\n                      <h3>분위기</h3>\r\n                            <input type=\"checkbox\" id=\"calm\" value=\"조용한\" v-model=\"atmosphere\" v-on:click= \"check_atmosphere()\">\r\n                            <label for=\"calm\">조용한</label>\r\n                            <input type=\"checkbox\" id=\"comfortable\" value=\"아늑한\" v-model=\"atmosphere\" v-on:click= \"check_atmosphere()\">\r\n                            <label for=\"comfortable\">아늑한</label>\r\n                            <input type=\"checkbox\" id=\"warm\" value=\"따뜻한\" v-model=\"atmosphere\" v-on:click= \"check_atmosphere()\">\r\n                            <label for=\"warm\">따뜻한</label>\r\n                            <br>\r\n                            <span>체크 : {{atmosphere}}</span>\r\n                            <span></span>\r\n                      <h3>누구랑 가면 좋을까요?</h3>\r\n                            <input type=\"checkbox\" id=\"friend\" value=\"친구\" v-model=\"withWho\" v-on:click= \"check_with()\">\r\n                            <label for=\"friend\">친구</label>\r\n                            <input type=\"checkbox\" id=\"family\" value=\"가족\" v-model=\"withWho\" v-on:click= \"check_with()\">\r\n                            <label for=\"family\">가족</label>\r\n                            <input type=\"checkbox\" id=\"coworker\" value=\"동료\" v-model=\"withWho\" v-on:click= \"check_with()\">\r\n                            <label for=\"coworker\">동료</label>\r\n                            <br>\r\n                            <span>체크 : {{withWho}}</span>\r\n                    </div>\r\n                    <b-button\r\n                      style=\"width: 100%;height: 20%; color: cornflowerblue;background-color: aliceblue; margin-bottom: 10px;\"\r\n                      class=\"mt-3\" block @click=\"$bvModal.hide('bv-modal-example')\">닫기</b-button>\r\n            </b-modal> <br>\r\n\r\n            <h3>음식 사진 업로드</h3>\r\n            <!-- TODO : 이미지가 있는지 없는지 체크해서 보여주기 -->\r\n            <template v-if=\"this.profile != ''\">\r\n              <!-- {{this.profile}}  -->\r\n               \r\n               <img :src=\"profile\" style=\"max-width:30%\" >\r\n            </template>\r\n            <template v-else>\r\n                사진없어요\r\n            </template>\r\n\r\n\r\n            <h3>사진업로드 테스트</h3>\r\n            <div class=\"uploadProfile\">\r\n                <input type=\"file\" name=\"fileToUpload\" id=\"fileToUpload\" accept=\".gif, .jpg, .png\"\r\n                    @change=\"getProfileForm\"> <!--change을 통해서 파일의 변화를 감지 -->\r\n                <button @click=\"upload\">제출</button>\r\n            </div> <br>\r\n            <span contenteditable=\"true\" placeholder=\"음식점 주소를 등록해주세요.\">\r\n            음식점 주소 : {{address}} \r\n            </span><br>\r\n            <b-button id=\"show-btn\"\r\n                    style=\"width: 50%;height: 15%;color: cornflowerblue;background-color: aliceblue; margin-bottom: 10px;\"\r\n                    @click=\"$bvModal.show('bv-modal-example_adr')\">음식점 주소 등록하기</b-button>\r\n                  <b-modal style=\"text-align: center; margin-bottom: 10px;\" id=\"bv-modal-example_adr\" hide-footer>\r\n                    <template v-slot:modal-title>음식점 주소 등록하기</template>\r\n                    <div class=\"d-block text-center\">\r\n                      <!--Main Modal Page-->\r\n                      <b-form-input id=\"address_search\" @keyup =\"fetchAdr\" type=\"text\"  v-model=\"address\" list=\"maps\"></b-form-input>\r\n                        <b-table striped hover :items=\"maps\" :click=\"test()\"></b-table>\r\n                    </div>\r\n                    <b-button\r\n                      style=\"width: 100%;height: 20%; color: cornflowerblue;background-color: aliceblue; margin-bottom: 10px;\"\r\n                      class=\"mt-3\" block @click=\"$bvModal.hide('bv-modal-example_adr')\">닫기</b-button>\r\n            </b-modal>\r\n            <br>\r\n            <!-- 음식점 주소 등록\r\n            <b-form-input id=\"address_search\" @keyup =\"fetchAdr\" class=\"in\" type=\"text\"  v-model=\"address\" list=\"maps\"></b-form-input>\r\n                <datalist  id=\"maps\"> \r\n            <option v-for=\"map in maps\" v-bind:key=\"map\">{{map}}</option>\r\n            </datalist><br>\r\n            <label for=\"address_search\"></label>  -->\r\n\r\n\r\n            평점 : <star-rating v-model=\"rating\"  :border-width=\"4\" border-color=\"#d8d8d8\" :rounded-corners=\"true\" :star-points=\"[23,2, 14,17, 0,19, 10,34, 7,50, 23,43, 38,50, 36,34, 46,19, 31,17]\"></star-rating><br>\r\n            장점 : <textarea v-model=\"cons\" class=\"cons_textarea\"  placeholder=\"장점을 작성해주세요~\"></textarea> <br>\r\n            단점 : <textarea v-model=\"props\" class=\"props_textarea\" placeholder=\"단점을 작성해주세요~\" ></textarea><br>\r\n\r\n             맛 : <star-rating v-model=\"flavor\"  :border-width=\"4\" border-color=\"#d8d8d8\" :rounded-corners=\"true\" :star-points=\"[23,2, 14,17, 0,19, 10,34, 7,50, 23,43, 38,50, 36,34, 46,19, 31,17]\"></star-rating> <br>\r\n             가격 : <star-rating v-model=\"price\"  :border-width=\"4\" border-color=\"#d8d8d8\" :rounded-corners=\"true\" :star-points=\"[23,2, 14,17, 0,19, 10,34, 7,50, 23,43, 38,50, 36,34, 46,19, 31,17]\"></star-rating> <br>\r\n             친절함 : <star-rating v-model=\"kindness\"  :border-width=\"4\" border-color=\"#d8d8d8\" :rounded-corners=\"true\" :star-points=\"[23,2, 14,17, 0,19, 10,34, 7,50, 23,43, 38,50, 36,34, 46,19, 31,17]\"></star-rating> <br>\r\n             <input type=\"submit\" name=\"submit\" id=\"submit\" class=\"form-submit\" value=\"review 등록\" />\r\n            \r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport '../../assets/css/style.scss'\r\nimport '../../assets/css/user.scss'\r\nimport UserApi from '../../apis/UserApi'\r\n\r\nimport ImgurApi from '../../apis/ImgurApi'\r\nimport StarRating from 'vue-star-rating'\r\n\r\nexport default {\r\n    created() {\r\n      this.getProfile();\r\n    },\r\n    computed() {\r\n        this.rating; //issue\r\n        console.log(this.rating);\r\n    },\r\n\r\n    components: {\r\n        StarRating\r\n    },\r\n    methods : {\r\n        check_area(){\r\n            this.area = [];\r\n        },\r\n        check_age(){\r\n            this.age = [];\r\n            \r\n        },\r\n        check_gender(){\r\n            this.gender = [];\r\n        },\r\n        check_atmosphere(){\r\n            this.atmosphere = [];\r\n        },\r\n        check_with(){\r\n            this.withWho = [];\r\n        },\r\n        getProfile(){\r\n                // Axios로 사진 가져오기\r\n                // console.log('프로필 가져오기!!!')\r\n\r\n                let email = \"ssafy@naver.com\";\r\n                let data = {\r\n                        email\r\n                    }\r\n                UserApi.requestProfile( email,res=>{\r\n                    // console.log(res)\r\n                    this.profile = res.object.profile\r\n                    // console.log(this.profile);\r\n                    console.log('프로필 가져오기 성공')\r\n                        \r\n                    },error=>{  \r\n                        // console.log('프로필 가져오기 실패')\r\n                    })\r\n            },\r\n            upload(){\r\n                console.log('이미지 업로드 @@')\r\n                // console.log(this.selectedImage)\r\n\r\n\r\n                ImgurApi.uploadProfile(this.selectedImage, res =>{\r\n                    // img url - res.link에 저장\r\n                     // 2) Imgur에 저장된 사진 링크를 가져오기\r\n                    \r\n                    this.imageUrl = res.data.link\r\n                    // this.imageUrl = \"https://i.imgur.com/91WnlBF.png\" // ######TEST 용\r\n                    // console.log(this.imageUrl)\r\n                    this.email = \"ssafy@naver.com\" // ######TEST 용\r\n                    \r\n                    \r\n                    // 3) 사진링크를 User의 profile 링크로 수정하기\r\n                    UserApi.requestUpload(this.email, this.imageUrl, res =>{\r\n                        // status로 판단\r\n                        console.log(res)\r\n                        if(res.status == true)\r\n                            console.log('프로필 업로드 성공!')\r\n                        this.getProfile();\r\n                    }, error =>{\r\n                        alert('프로필 업로드 실패')\r\n                    })\r\n\r\n                }, error =>{\r\n                    alert('Imgur 업로드 실패!')\r\n                })\r\n                \r\n            },\r\n            getProfileForm(event){\r\n                this.selectedImage = event.target.files[0];\r\n            },\r\n            setRating(rating) {\r\n                this.rating = \"You have Selected: \" + rating + \" stars\";\r\n                console.log(this.rating);\r\n             },\r\n             fetchAdr(){\r\n               \r\n                this\r\n                .$store\r\n                .dispatch('FETCH_ADR');\r\n                \r\n             },test(){\r\n                 console.log('hi')\r\n             }\r\n    },\r\n    data : () =>({\r\n         fixArea : ['강남','종로','잠실','이태원'],\r\n         area : [],\r\n         age : [],\r\n         gender : [],\r\n         atmosphere : [],\r\n         withWho : [],\r\n         profile: '',\r\n         mageUrl: '',\r\n         selectedImage: '',\r\n         rating : '',\r\n         cons : '',\r\n         props : '',\r\n         flavor: '',\r\n         price : '',\r\n         kindness : '',\r\n         maps :[\r\n          { isActive: true, age: 40, first_name: 'Dickerson', last_name: 'Macdonald' },\r\n          { isActive: false, age: 21, first_name: 'Larsen', last_name: 'Shaw' },\r\n          { isActive: false, age: 89, first_name: 'Geneva', last_name: 'Wilson' },\r\n          { isActive: true, age: 38, first_name: 'Jami', last_name: 'Carney' }\r\n        ], //Unit test\r\n         address : '',\r\n    })\r\n}\r\n</script>\r\n\r\n<style>\r\n.cons_textarea{\r\n    border: 1px solid #3f0808;\r\n    padding: 10px 100px;\r\n    margin-right: 10px;\r\n    margin-top: 20px;\r\n    margin-bottom: 20px;\r\n}\r\n.props_textarea{\r\n    border: 1px solid #3f0808;\r\n    padding: 10px 100px;\r\n    margin-right: 10px;\r\n    margin-top: 20px;\r\n    margin-bottom: 20px;\r\n}\r\n\r\n</style>"],"sourceRoot":"src/views/review"}]}