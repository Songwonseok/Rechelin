,EAAA,kBAAA,CAAA,EAAA;AACA,WAAA,SAAA;AACA,KAHA;AAIA,IAAA,GAAA,EAAA,aAAA,CAAA,EAAA;AACA,WAAA,SAAA;AACA,KANA;AAOA,IAAA,GAAA,EAAA,aAAA,CAAA,EAAA;AACA,WAAA,SAAA;AACA,KATA;AAUA,IAAA,KAAA,EAAA,eAAA,CAAA,EAAA;AACA,WAAA,SAAA;AACA;AAZA,GApBA;AAkCA,EAAA,QAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,aAAA,KAAA,MAAA,CAAA,MAAA,CAAA,IAAA;AACA,KAHA,CAIA;;AAJA,GAlCA;AAwCA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,uBACA;AACA;AACA;AAEA,UAAA,KAAA,GAAA,CAAA,MAAA,GAAA,CAAA,IAAA,CAAA,KAAA,cAAA,CAAA,QAAA,CAAA,KAAA,GAAA,CAAA,EAAA;AACA,aAAA,KAAA,CAAA,GAAA,GAAA,yBAAA;AACA,OAFA,MAEA,KAAA,KAAA,CAAA,EAAA,GAAA,KAAA;;AACA,UAAA,KAAA,GAAA,CAAA,MAAA,GAAA,CAAA,IAAA,KAAA,GAAA,IAAA,KAAA,GAAA,EAAA;AACA,aAAA,KAAA,CAAA,SAAA,GAAA,iBAAA;AACA,OAFA,MAEA,KAAA,KAAA,CAAA,SAAA,GAAA,KAAA;;AACA,UAAA,OAAA,GAAA,IAAA;AACA,MAAA,MAAA,CAAA,MAAA,CAAA,KAAA,KAAA,EAAA,GAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,EAAA;AACA,UAAA,OAAA,GAAA,KAAA;AACA;AACA,OAJA;AAKA,WAAA,QAAA,GAAA,OAAA;AAEA,KAnBA;AAoBA,IAAA,IApBA,kBAoBA;AAAA;;AAEA,UAAA,KAAA,QAAA,EAAA;AAAA,YAEA,KAFA,GAOA,IAPA,CAEA,KAFA;AAAA,YAGA,QAHA,GAOA,IAPA,CAGA,QAHA;AAAA,YAIA,GAJA,GAOA,IAPA,CAIA,GAJA;AAAA,YAKA,KALA,GAOA,IAPA,CAKA,KALA;AAAA,YAMA,KANA,GAOA,IAPA,CAMA,KANA;AAQA,YAAA,IAAA,GAAA;AACA,mBAAA,KADA;AAEA,sBAAA,QAFA;AAGA,gBAAA,GAHA;AAIA,mBAAA,KAJA;AAKA,qBAAA;AALA,SAAA;AAOA,aAAA,QAAA,GAAA,KAAA;AACA,QAAA,OAAA,CAAA,WAAA,CAAA,IAAA,EAAA,UAAA,GAAA,EAAA;AAEA,UAAA,KAAA,CAAA,cAAA,CAAA;;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,YAAA,IAAA;AADA,WAAA;AAGA,SANA,EAMA,UAAA,KAAA,EAAA;AAEA,UAAA,OAAA,CAAA,GAAA,CAAA,iBAAA;AACA,UAAA,KAAA,CAAA,gBAAA,CAAA,CAHA,CAIA;;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,KAAA;AACA,SAZA,EAhBA,CA6BA;AACA;AACA,KArDA;AAsDA;AACA,IAAA,IAvDA,gBAuDA,CAvDA,EAuDA;AACA,UAAA,KAAA,GAAA,CAAA,CAAA,MAAA,CAAA,KAAA,IAAA,CAAA,CAAA,YAAA,CAAA,KAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,CAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,UAAA,CAAA,KAAA,CAAA,MAAA,EAAA;AACA,WAAA,WAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,CAAA,CAAA,CAAA,WAAA;AACA,KA7DA;AA8DA,IAAA,WA9DA,uBA8DA,IA9DA,EA8DA;AAAA;;AACA,UAAA,KAAA,GAAA,IAAA,KAAA,EAAA;AACA,UAAA,MAAA,GAAA,IAAA,UAAA,EAAA;;AAEA,MAAA,MAAA,CAAA,MAAA,GAAA,UAAA,CAAA,EAAA;AAEA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,GAAA,CAAA,CAAA,MAAA,CAAA,MAAA,CAFA,CAGA;;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA,CAAA,MAAA,CAAA,MAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,CAAA,CAAA,MAAA,CAAA,MAAA;AACA,OANA;;AAOA,MAAA,MAAA,CAAA,aAAA,CAAA,IAAA;AACA;AA1EA,GAxCA;AAqHA,EAAA,OArHA,qBAqHA;AACA,SAAA,cAAA,CAAA;AAAA,KACA,EADA,GACA,GADA,CACA,CADA,EAEA,EAFA,GAEA,GAFA,CAEA,GAFA,EAGA,GAHA,GAGA,MAHA,GAIA,GAJA,GAIA,OAJA;AAKA;AA3HA,CAAA","sourcesContent":["<template>\r\n    <div style=\"text-align:center;\">\r\n\r\n        <h1 style=\"margin-top: 20px; margin-bottom: 20px;\">회원정보 수정 페이지</h1>\r\n\r\n        <div>\r\n            <img :src=\"image\" :alt=\"EditUser.name\" style=\"height: 300px; width:300px;\">\r\n\r\n            <input type=\"file\" @change=\"load\" accept=\".jpg, .jpeg, .png, .gif\">\r\n\r\n\r\n        </div>\r\n\r\n        <div style=\"margin-top: 10px;\">\r\n            닉네임 변경 : <input v-model=\"nickname\" type=\"text\" id=\"nickname\" @keyup.enter=\"edit\"\r\n                :placeholder=\"EditUser.name\">\r\n        </div>\r\n\r\n        <div style=\"margin-top: 10px;\">\r\n            비밀번호 변경: <input v-model=\"pw1\" type=\"password\" id=\"pw1\" @keyup.enter=\"edit\">\r\n        </div>\r\n        <div v-if=\"error.pw1\">{{error.pw1}}</div>\r\n\r\n        <div style=\"margin-top: 10px;\">\r\n            비밀번호 확인: <input v-model=\"pw2\" type=\"password\" id=\"pw2\" @keyup.enter=\"edit\">\r\n            <div v-if=\"error.confirmPW\">{{error.confirmPW}}</div>\r\n        </div>\r\n\r\n        <div style=\"margin-top: 10px;\">\r\n            핸드폰 번호 변경: <input v-model=\"phone\" type=\"text\" id=\"phone\" :placeholder=\"EditUser.phone\">\r\n        </div>\r\n        <v-btn class=\"ma-2\" tile outlined color=\"success\" @click=\"edit\">\r\n            <v-icon left>{{ svgPath }}</v-icon>\r\n            EDIT\r\n        </v-btn>\r\n\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import PV from 'password-validator'\r\n    import UserApi from '../../apis/UserApi'\r\n    import router from '../../routes.js'\r\n    import {\r\n        mdiPencil\r\n    } from '@mdi/js'\r\n\r\n    export default {\r\n        data() {\r\n            return {\r\n                email: this.$route.params.info.email,\r\n                nickname: this.$route.params.info.name,\r\n                pw1: this.$route.params.info.pw1,\r\n                pw2: this.$route.params.info.pw1,\r\n                phone: this.$route.params.info.phone,\r\n                image: this.$route.params.info.picture,\r\n                passwordSchema: new PV(),\r\n                error: {\r\n                    pw1: false,\r\n                    confirmPW: false\r\n                },\r\n\r\n                isSubmit: true,\r\n                svgPath: mdiPencil\r\n\r\n            }\r\n        },\r\n        watch: {\r\n            nickname: function (v) {\r\n                this.checkForm();\r\n            },\r\n            pw1: function (v) {\r\n                this.checkForm();\r\n            },\r\n            pw2: function (v) {\r\n                this.checkForm();\r\n            },\r\n            phone: function (v) {\r\n                this.checkForm();\r\n            }\r\n        },\r\n        computed: {\r\n            EditUser() {\r\n                return this.$route.params.info\r\n            }\r\n            // 해당 회원이 자기 자신인지 아닌지 확인하는 함수 만들기 : button에 v-if!만들기\r\n        },\r\n        methods: {\r\n            checkForm() {\r\n                // 기존에 있는 닉네임인지\r\n                // 기존에 등록된 휴대폰 번호인지 확인\r\n\r\n                if (this.pw1.length > 0 && !this.passwordSchema.validate(this.pw1)) {\r\n                    this.error.pw1 = '영문, 숫자 포함 8자리 이상이어야 합니다'\r\n                } else this.error.PW = false;\r\n                if (this.pw2.length > 0 && this.pw1 != this.pw2) {\r\n                    this.error.confirmPW = '비밀번호가 일치하지 않습니다'\r\n                } else this.error.confirmPW = false;\r\n                let Editing = true;\r\n                Object.values(this.error).map(err => {\r\n                    if (err) {\r\n                        Editing = false;\r\n                    }\r\n                })\r\n                this.isSubmit = Editing;\r\n\r\n            },\r\n            edit() {\r\n\r\n                if (this.isSubmit) {\r\n                    let {\r\n                        email,\r\n                        nickname,\r\n                        pw1,\r\n                        phone,\r\n                        image\r\n                    } = this;\r\n                    let data = {\r\n                        \"email\": email,\r\n                        \"nickname\": nickname,\r\n                        \"pw\": pw1,\r\n                        \"phone\": phone,\r\n                        \"profile\": image\r\n                    }\r\n                    this.isSubmit = false;\r\n                    UserApi.requestEdit(data, res => {\r\n\r\n                        alert('회원 정보 수정에 성공');\r\n                        this.$router.push({\r\n                            name: `UserPage`\r\n                        })\r\n                    }, error => {\r\n\r\n                        console.log(\"회원 정보 수정 실패 !!!\");\r\n                        alert('회원 정보 수정 실패 !!')\r\n                        // 로그인 실패시 버튼 비활성화\r\n                        this.isSubmit = false;\r\n                    })\r\n                    // axios 보낸는 장소\r\n                }\r\n            },\r\n            // 프로필 이미지 변경\r\n            load(e) {\r\n                var files = e.target.files || e.dataTransfer.files;\r\n                console.log(e.target.files[0])\r\n                if (!files.length) return;\r\n                this.createImage(files[0]);\r\n                console.log(files[0].mozFullPath)\r\n            },\r\n            createImage(file) {\r\n                let image = new Image();\r\n                let reader = new FileReader();\r\n\r\n                reader.onload = (e) => {\r\n\r\n                    this.EditUser.picture = e.target.result;\r\n                    // 파일 경로\r\n                    console.log(e.target.result)\r\n                    this.image = e.target.result;\r\n                }\r\n                reader.readAsDataURL(file);\r\n            }\r\n\r\n        },\r\n        created() {\r\n            this.passwordSchema // 비밀번호 형식 체크\r\n                .is().min(8)\r\n                .is().max(100)\r\n                .has().digits()\r\n                .has().letters();\r\n        }\r\n    }\r\n</script>"],"sourceRoot":"src/views/userpage"}]}