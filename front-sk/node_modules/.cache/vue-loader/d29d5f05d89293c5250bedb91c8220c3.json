\r\n      if (val !== this.innerValue) {\r\n        this.innerValue = val;\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    if (this.value) {\r\n      this.innerValue = this.value;\r\n    }\r\n  }\r\n};\r\n\r\n</script>\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n.TextInput {\r\n  padding-bottom: 18px;\r\n  input {\r\n    z-index: 99999;\r\n    padding-top: 1.4rem;\r\n\r\n    &.has-value,\r\n    &:focus {\r\n      outline: none;\r\n    }\r\n  }\r\n\r\n  label {\r\n    margin-top: 1rem;\r\n    user-select: none;\r\n  }\r\n\r\n  input.has-value ~ label,\r\n  input:focus ~ label {\r\n    font-size: 0.6rem;\r\n    margin-top: 0;\r\n    transition: all 0.2s ease-in-out;\r\n  }\r\n}\r\n.TextInput input[data-v-29d5bae9] {\r\n    z-index: 99999;\r\n    padding-top: 1.4rem;\r\n    width: 100%;\r\n}\r\n.TextInput[data-v-29d5bae9] {\r\n    padding-bottom: 18px;\r\n    margin-bottom: 1rem;\r\n}\r\n.btn-secondary {\r\n    width: auto;\r\n    width: 100%;\r\n    color: #3480b6;\r\n    background-color: #6c757d;\r\n    border-color: #6c757d;\r\n    width: 100%;\r\n    margin-bottom: 10px;\r\n    text-align: center;\r\n}\r\n\r\n</style>\r\n"]}]}