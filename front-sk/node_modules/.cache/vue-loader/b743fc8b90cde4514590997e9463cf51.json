 ÏûàÎäîÏßÄ ÏóÜÎäîÏßÄ Ï≤¥ÌÅ¨Ìï¥ÏÑú Î≥¥Ïó¨Ï£ºÍ∏∞ -->\r\n            <template v-if=\"this.profile != ''\">\r\n            ÏÇ¨ÏßÑÏûàÏñ¥Ïöî \r\n            <!-- {{this.profile}} -->\r\n               <img :src=\"profile\" style=\"max-width:30%\" >\r\n\r\n            </template>\r\n            <template v-else>\r\n                ÏÇ¨ÏßÑÏóÜÏñ¥Ïöî\r\n            </template>\r\n\r\n\r\n            <h3>ÏÇ¨ÏßÑÏóÖÎ°úÎìú ÌÖåÏä§Ìä∏</h3>\r\n            <div class=\"uploadProfile\">\r\n                <input type=\"file\" name=\"fileToUpload\" id=\"fileToUpload\" accept=\".gif, .jpg, .png\"\r\n                    @change=\"getProfileForm\"> <!--changeÏùÑ ÌÜµÌï¥ÏÑú ÌååÏùºÏùò Î≥ÄÌôîÎ•º Í∞êÏßÄ -->\r\n                <button @click=\"upload\">Ï†úÏ∂ú</button>\r\n            </div>\r\n\r\n\r\n            <h1 >Î°úÍ∑∏Ïù∏ Ìï¥Ï£ºÏÑ∏Ïöî! <br>Ïò§ÎäòÎèÑ Ï¶êÍ≤ÅÍ≤å ü§£</h1>\r\n\r\n\r\n            <div class=\"input-with-label\">\r\n                <input v-model=\"email\" v-bind:class=\"{error : error.email, complete:!error.email&&email.length!==0}\"\r\n                       @keyup.enter=\"login\"\r\n                       id=\"email\" placeholder=\"Ïù¥Î©îÏùºÏùÑ ÏûÖÎ†•ÌïòÏÑ∏Ïöî.\"\r\n                       type=\"text\" style=\"\r\n            background-color: white;\r\n            color: black;\r\n            border: 2px solid #008Cy\r\n            BA;\"/>\r\n                <label for=\"email\">Ïù¥Î©îÏùº</label>\r\n                <div class=\"error-text\" v-if=\"error.email\">\r\n                    {{error.email}}\r\n                </div>\r\n            </div>\r\n\r\n            <div class=\"input-with-label\">\r\n                <input v-model=\"password\" type=\"password\"\r\n                       v-bind:class=\"{error : error.password, complete:!error.password&&password.length!==0}\"\r\n                       id=\"password\"\r\n                       @keyup.enter=\"login\"\r\n                       placeholder=\"ÎπÑÎ∞ÄÎ≤àÌò∏Î•º ÏûÖÎ†•ÌïòÏÑ∏Ïöî.\" style=\"background-color: white; color: black; border: 2px solid rgb(0, 140, 186);\"/>\r\n                <label for=\"password\">ÎπÑÎ∞ÄÎ≤àÌò∏</label>\r\n                <div class=\"error-text\" v-if=\"error.password\">\r\n                    {{error.password}}\r\n\r\n                </div>\r\n            </div>\r\n            <button class=\"btn btn--back btn--login\" v-on:click=\"login\" :disabled=\"!isSubmit\"\r\n                    :class=\"{disabled : !isSubmit}\">\r\n                Î°úÍ∑∏Ïù∏\r\n\r\n            </button>\r\n\r\n\r\n            <div class=\"sns-login\" style=\"\r\n    text-align: center;\">\r\n                <div class=\"text\">\r\n                    <p>SNS Í∞ÑÌé∏ Î°úÍ∑∏Ïù∏</p>\r\n                    <div class=\"bar\"></div>\r\n                </div>\r\n\r\n\r\n                <!-- ÏÜåÏÖú Î°úÍ∑∏Ïù∏ -->\r\n                <NaverLogin :component=\"component\" />       \r\n                <!-- <kakaoLogin :component=\"component\"/> -->\r\n                <!-- <GoogleLogin :component=\"component\"/> -->\r\n\r\n            </div>\r\n            <div class=\"add-option\">\r\n                <div class=\"text\">\r\n                    \r\n                    <div class=\"bar\"></div>\r\n                </div>\r\n                <div class=\"wrap\">\r\n                    <p>ÎπÑÎ∞ÄÎ≤àÌò∏Î•º ÏûäÏúºÏÖ®ÎÇòÏöî? </p>\r\n                    <router-link v-bind:to=\"{name:'searchPassword'}\" class=\"btn--text\">üëâÎπÑÎ∞ÄÎ≤àÌò∏ Ï∞æÍ∏∞</router-link>\r\n                </div>\r\n                <div class=\"wrap\">\r\n                    <p>ÏïÑÏßÅ ÌöåÏõêÏù¥ ÏïÑÎãàÏã†Í∞ÄÏöî? </p>\r\n                    <router-link v-bind:to=\"{name:'signUpForm'}\" class=\"btn--text\">üëâÍ∞ÄÏûÖÌïòÍ∏∞</router-link>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n     /*eslint-disable*/\r\n    import '../../assets/css/style.scss'\r\n    import '../../assets/css/user.scss'\r\n    import PV from 'password-validator'\r\n    import * as EmailValidator from 'email-validator';\r\n    import KakaoLogin from '../../components/user/snsLogin/Kakao.vue'\r\n    import GoogleLogin from '../../components/user/snsLogin/Google.vue'\r\n    import NaverLogin from '../../components/user/snsLogin/Naver.vue'\r\n    import UserApi from '../../apis/UserApi'\r\n    import JoinPage from './Join.vue';\r\n    import ImgurApi from '../../apis/ImgurApi'\r\n\r\n    export default {\r\n        components: {\r\n            KakaoLogin,\r\n            GoogleLogin,\r\n            JoinPage,\r\n            NaverLogin,\r\n        },\r\n        created(){\r\n\r\n            this.component = this;\r\n\r\n\r\n            this.passwordSchema\r\n                .is().min(8)\r\n                .is().max(100)\r\n                .has().digits()\r\n                .has().letters();\r\n                \r\n            this.getProfile();\r\n        },\r\n        watch: {\r\n            password: function (v) {\r\n                this.checkForm();\r\n            },\r\n            email: function (v) {\r\n                this.checkForm();\r\n            },\r\n        },\r\n        methods: {\r\n            checkForm(){\r\n                if (this.email.length >= 0 && !EmailValidator.validate(this.email))\r\n                    this.error.email = \"Ïù¥Î©îÏùº ÌòïÏãùÏù¥ ÏïÑÎãôÎãàÎã§.\"\r\n                else this.error.email = false;\r\n\r\n                if (this.password.length >= 0 && !this.passwordSchema.validate(this.password))\r\n                    this.error.password = 'ÏòÅÎ¨∏,Ïà´Ïûê Ìè¨Ìï® 8 ÏûêÎ¶¨Ïù¥ÏÉÅÏù¥Ïñ¥Ïïº Ìï©ÎãàÎã§.'\r\n                else\r\n                    this.error.password = false;\r\n\r\n\r\n                let isSubmit = true;\r\n                Object.values(this.error).map(v => {\r\n                    if (v) isSubmit = false;\r\n                })\r\n                this.isSubmit = isSubmit;\r\n            }\r\n            ,login(){\r\n\r\n                if (this.isSubmit) {    \r\n                    let {email,password} = this;\r\n                    let data = {\r\n                        email,password\r\n                    }\r\n\r\n                    //ÏöîÏ≤≠ ÌõÑÏóêÎäî Î≤ÑÌäº ÎπÑÌôúÏÑ±Ìôî\r\n                    this.isSubmit = false;\r\n                    UserApi.requestLogin( data,res=>{\r\n                        //ÌÜµÏã†ÏùÑ ÌÜµÌï¥ Ï†ÑÎã¨Î∞õÏùÄ Í∞í ÏΩòÏÜîÏóê Ï∂úÎ†•\r\n                        console.log(res);\r\n                        if(res.data == \"success\")    \r\n                            this.$router.push({ name: \"Main\" });\r\n                        else\r\n                            this.$router.push({ path: '/signUpForm' });\r\n                        //ÏöîÏ≤≠Ïù¥ ÎÅùÎÇòÎ©¥ Î≤ÑÌäº ÌôúÏÑ±Ìôî\r\n                        this.isSubmit = true;\r\n                    },error=>{  \r\n                        this.isSubmit = true;\r\n                    })\r\n                }\r\n\r\n                console.log('Î°úÍ∑∏Ïù∏ ÎÅù')\r\n\r\n            },\r\n            getProfile(){\r\n                // AxiosÎ°ú ÏÇ¨ÏßÑ Í∞ÄÏ†∏Ïò§Í∏∞\r\n                // console.log('ÌîÑÎ°úÌïÑ Í∞ÄÏ†∏Ïò§Í∏∞!!!')\r\n\r\n                let email = \"ssafy@naver.com\";\r\n                let data = {\r\n                        email\r\n                    }\r\n                UserApi.requestProfile( email,res=>{\r\n                    // console.log(res)\r\n                    this.profile = res.object.profile\r\n                    // console.log(this.profile);\r\n                    // console.log('ÌîÑÎ°úÌïÑ Í∞ÄÏ†∏Ïò§Í∏∞ ÏÑ±Í≥µ')\r\n                        \r\n                    },error=>{  \r\n                        // console.log('ÌîÑÎ°úÌïÑ Í∞ÄÏ†∏Ïò§Í∏∞ Ïã§Ìå®')\r\n                    })\r\n            },\r\n            upload(){\r\n                console.log('Ïù¥ÎØ∏ÏßÄ ÏóÖÎ°úÎìú @@')\r\n                // console.log(this.selectedImage)\r\n\r\n\r\n                ImgurApi.uploadProfile(this.selectedImage, res =>{\r\n                    // img url - res.linkÏóê Ï†ÄÏû•\r\n                     // 2) ImgurÏóê Ï†ÄÏû•Îêú ÏÇ¨ÏßÑ ÎßÅÌÅ¨Î•º Í∞ÄÏ†∏Ïò§Í∏∞\r\n                    \r\n                    // this.imageUrl = res.data.link\r\n                    this.imageUrl = \"https://i.imgur.com/91WnlBF.png\" // ######TEST Ïö©\r\n                    // console.log(this.imageUrl)\r\n                    this.email = \"ssafy@naver.com\" // ######TEST Ïö©\r\n                    \r\n                    \r\n                    // 3) ÏÇ¨ÏßÑÎßÅÌÅ¨Î•º UserÏùò profile ÎßÅÌÅ¨Î°ú ÏàòÏ†ïÌïòÍ∏∞\r\n                    UserApi.requestUpload(this.email, this.imageUrl, res =>{\r\n                        // statusÎ°ú ÌåêÎã®\r\n                        console.log(res)\r\n                        if(res.status == true)\r\n                            console.log('ÌîÑÎ°úÌïÑ ÏóÖÎ°úÎìú ÏÑ±Í≥µ!')\r\n                    }, error =>{\r\n                        alert('ÌîÑÎ°úÌïÑ ÏóÖÎ°úÎìú Ïã§Ìå®')\r\n                    })\r\n\r\n                }, error =>{\r\n                    alert('Imgur ÏóÖÎ°úÎìú Ïã§Ìå®!')\r\n                })\r\n                \r\n            },\r\n            getProfileForm(event){\r\n                this.selectedImage = event.target.files[0];\r\n            }\r\n            \r\n        },\r\n        data: () => {\r\n            return {\r\n                email: '',\r\n                password: '',\r\n                passwordSchema: new PV(),\r\n                error: {\r\n                    email: false,\r\n                    passowrd: false,\r\n                },\r\n                isSubmit: false,\r\n                component: this,\r\n                profile: '',\r\n                imageUrl: '',\r\n                selectedImage: ''\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n.btn.disabled {\r\n    /* background: #ccc; */\r\n    /* display: inline-block; */\r\n    padding: 3px 25px;\r\n    font-size: 20px;\r\n    cursor: pointer;\r\n    text-align: center;\r\n    text-decoration: none;\r\n    outline: none;\r\n    color: #fff;\r\n    background-color: #FF9800;\r\n    border: none;\r\n    border-radius: 20px;\r\n    box-shadow: 0 9px #999;\r\n}\r\n\r\n.user#login .text .bar {\r\n    height: 1px;\r\n    background-color: #238bde;\r\n    float: right;\r\n    margin-top: 9px;\r\n    margin-bottom: 20px;\r\n}\r\n.user#login .add-option p {\r\n    float: initial;\r\n}\r\n\r\n.user#login .add-option a {\r\n    color: #000;\r\n    float: unset;\r\n    font-weight: 600;\r\n}\r\n.btn--back{\r\n    border-radius: 3px;\r\n    padding: 3px 25px;\r\n    font-size: 20px;\r\n    cursor: pointer;\r\n    text-align: center;\r\n    text-decoration: none;\r\n    outline: none;\r\n    color: #fff;\r\n    background-color: #238bde;\r\n    border: none;\r\n    border-radius: 20px;\r\n}\r\n</style>\r\n"]}]}